{
  "name": "@yuva-devlab/eslint-config",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Shared ESLint configuration",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./base": {
      "types": "./dist/base.d.ts",
      "import": "./dist/base.js",
      "require": "./dist/base.cjs"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.js",
      "require": "./dist/node.cjs"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js",
      "require": "./dist/react.cjs"
    },
    "./typescript": {
      "types": "./dist/typescript.d.ts",
      "import": "./dist/typescript.js",
      "require": "./dist/typescript.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "clean:build": "rm -rf dist",
    "clean:node_modules": "rm -rf node_modules",
    "clean:turbo": "rm -rf .turbo",
    "clean": "pnpm run clean:build && pnpm run clean:node_modules && pnpm run clean:turbo"
  },
  "dependencies": {
    "@eslint/js": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "typescript-eslint": "^8.47.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/node": "^24.10.1",
    "@yuva-devlab/typescript-config": "workspace:*",
    "eslint": "^9.39.1",
    "tsup": "^8.5.1"
  },
  "peerDependencies": {
    "eslint": "^9.0.0"
  }
}
