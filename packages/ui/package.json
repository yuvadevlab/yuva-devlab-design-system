{
  "name": "@yuva-devlab/ui",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "**/*.css.ts"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/ui.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "storybook": "pnpm --filter @yuva-devlab/docs storybook",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "clean:build": "rm -rf dist",
    "clean:node_modules": "rm -rf node_modules",
    "clean:turbo": "rm -rf .turbo",
    "clean": "pnpm run clean:build && pnpm run clean:node_modules && pnpm run clean:turbo"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.17.4",
    "@yuva-devlab/colors": "workspace:*",
    "@yuva-devlab/primitives": "workspace:*",
    "@yuva-devlab/tokens": "workspace:*",
    "clsx": "^2.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.6",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/expect": "^4.0.13",
    "@yuva-devlab/eslint-config": "workspace:*",
    "@yuva-devlab/prettier-config": "workspace:*",
    "@yuva-devlab/typescript-config": "workspace:*",
    "jsdom": "^27.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.13"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}
