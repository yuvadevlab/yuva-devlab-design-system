{
  "name": "@yuva-devlab/primitives",
  "version": "0.1.2",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "license": "MIT",
  "scripts": {
    "build": "vite build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "clean:build": "rm -rf dist",
    "clean:node_modules": "rm -rf node_modules",
    "clean:turbo": "rm -rf .turbo",
    "clean": "pnpm run clean:build && pnpm run clean:node_modules && pnpm run clean:turbo"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4"
  },
  "devDependencies": {
    "@types/react": "^19.2.6",
    "@yuva-devlab/eslint-config": "workspace:*",
    "@yuva-devlab/prettier-config": "workspace:*",
    "@yuva-devlab/typescript-config": "workspace:*",
    "react": "^19.2.0",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "react": "^19.2.0"
  }
}
